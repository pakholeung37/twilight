{
  "name": "twilight",
  "version": "0.1.0",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "bootstrap": "lerna bootstrap --use-workspaces",
    "build": "lerna run build --stream",
    "test": "lerna run test --stream",
    "test:editor": "yarn workspace @twilight/editor test",
    "start": "yarn workspace @twilight/editor start",
    "storybook": "start-storybook -s ./packages/editor/public -p 6006",
    "build-storybook": "build-storybook"
  },
  "dependencies": {
    "@emotion/babel-plugin": "^11.0.0",
    "@emotion/babel-preset-css-prop": "^11.0.0",
    "@emotion/eslint-plugin": "^11.0.0",
    "@storybook/addon-actions": "^6.1.8",
    "@storybook/addon-essentials": "^6.1.8",
    "@storybook/addon-links": "^6.1.8",
    "@storybook/react": "^6.1.8",
    "@types/color-string": "^1.5.0",
    "@types/color-convert": "^2.0.0",
    "@types/jest": "^26.0.15",
    "@types/node": "^12.0.0",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-router-dom": "^5.1.6",
    "@typescript-eslint/eslint-plugin": "^4.5.0",
    "@typescript-eslint/parser": "^4.5.0",
    "eslint": "^7.11.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "jest": "26.6.0",
    "jest-circus": "26.6.0",
    "jest-environment-jsdom-fourteen": "1.0.1",
    "jest-resolve": "26.6.0",
    "jest-watch-typeahead": "0.6.1",
    "lerna": "^3.22.1",
    "prettier": "^2.1.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-refresh": "^0.8.3",
    "typescript": "^4.4.0"
  },
  "devDependencies": {}
}
