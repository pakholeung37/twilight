{
  "parserOptions": {
    "ecmaVersion": 2019,
    "sourceType": "module"
  },
  "env": {
    "es6": true,
    "browser": true
  },
  "plugins": ["svelte3"],
  "settings": {
    "svelte3/ignore-styles": true
  },
  "overrides": [
    {
      "files": ["**/*.svel"],
      "processor": "svelte3/svelte3"
    }
  ],
  "ignorePatterns": ["coverage/"]
}
