    "jsx",
    "konva"
  ],
  "bugs": "https://github.com/konvajs/react-konva/issues",
  "main": "lib/ReactKonva.js",
  "repository": {
    "type": "git",
    "url": "git@github.com:konvajs/react-konva.git"
  },
  "dependencies": {
    "react-reconciler": "~0.26.0-rc.0",
    "scheduler": "^0.19.1"
  },
  "peerDependencies": {
    "konva": ">3.2.5",
    "react": ">=17.0.0",
    "react-dom": ">=17.0.0"
  },
  "devDependencies": {
    "@wojtekmaj/enzyme-adapter-react-17": "^0.1.1",
    "@types/react": "^16.9.34",
    "chai": "^4.2.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "konva": "^7.0.4",
    "nwb": "^0.23.0",
    "react": "^17.0.0",
    "react-dom": "^17.0.0",
    "sinon": "^7.5.0",
    "use-image": "^1.0.5"
  },
  "scripts": {
    "build": "nwb build-react-component && cpy ./ReactKonvaCore.d.ts ./lib",
